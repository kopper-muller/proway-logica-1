algoritmo "semnome"
// Função :
// Autor :
// Data : 11/04/2024
// Seção de Declarações 
var
numero1: inteiro
numero2: inteiro
numero3: inteiro

inicio
// Seção de Comandos 
escreval ("diga-me um número: ")
leia(numero1)
escreval ("diga-me outro número: ")
leia(numero2)
escreval ("diga-me um último número: ")
leia(numero3)

se ((numero1>=numero2) e (numero2>=numero3)) entao
   escreval(numero1, numero2, numero3)
senao
   se (numero1>=numero3) e (numero3>=numero2) entao
      escreval (numero1, numero3, numero2)
   senao
      se (numero2>=numero1) e (numero1>=numero3) entao
         escreval(numero2, numero1, numero3)
      senao
         se (numero2>=numero3) e (numero3>=numero1) entao
            escreval (numero2, numero3, numero1)
         senao
            se (numero3>=numero2) e (numero2>=numero1) entao
               escreval (numero3, numero2, numero1)
            senao
               se (numero3>=numero1) e (numero1>=numero2) entao
                  escreval (numero3, numero1, numero2)
fimse
fimse
fimse
fimse
fimse
fimse


fimalgoritmo
